{
  "name": "js-vite-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "compile:css": " sass --no-source-map src/scss/style.scss:src/css/styles.css --style expanded",
