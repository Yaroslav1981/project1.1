  },
  "dependencies": {
    "autoprefixer": "^10.4.16",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0"
  }
}
